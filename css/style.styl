// start function

back(path)
	background url(../img/path) center top no-repeat

// end function

#__bs_notify__
	display: none !important;

.wid
	width:1200px;
	margin:0 auto;



.mfzssz_path
	height 52px	
	line-height 52px	
	background #f9f9f9	
	font-size 12px	
	color #00001a	
	font-weight bold
	a
		font-weight normal
		margin-right 20px	
	.sech
		float right	
		width 260px
		height 26px
		border 1px solid #dcdcdc
		line-height 26px
		overflow hidden
		margin 13px 24px 0 0
		form
			width 100%
			height 100%
			input
				height 100%
				line-height 26px
				border none
			input[type="text"]
				width 80%
				float left
				background url("../img/6e1e244bc1e1cf96029bd3be45469a46.png") left center no-repeat
				padding-left 30px
			input[type="submit"]
				width 20%
				float right
				background #d70101
				color #fff
				font-size 12px
				border-radius 0
				

.mfzssz_select-box
	margin-bottom 30px
	.box
		width 100%
		border-bottom 1px dashed #e0e0e0
		line-height 30px
		padding-top 20px
		display table
		.left
			width 110px
			float left
		.right
			width calc(100% - 110px)
			float right
			span
				padding 0 12px
				float left
				margin 0 10px 15px
				cursor pointer
				&.acti
					background #9c2730
					color #fff


.mfzssz_show-box
	margin-bottom 180px
	&.mb
		margin-bottom 0
	.mfzssz_condition
		height 50px
		line-height 50px
		background #f2f2f2
		padding 0 25px
		font-size 16px
		color #222
		margin-bottom 15px
		span
			float right
			i
				color #95264f
	.mfzssz_box
		.box
			position relative
			width 100%
			height 310px
			margin-bottom 20px
			.pic
				width 100%
				height 260px
				overflow hidden
				a
					width 100%
					height 100%
					display block
					&:hover
						img
							transform scale(1.1)
							opacity .85
					img
						width 100%
						min-height 100%
						transition .3s
			.text
				position absolute
				bottom 0
				left 50%
				transform translateX(-50%)
				width 95%
				background #fff
				height 80px
				padding 20px 25px
				box-shadow 0 0 20px rgba(#000,.15)
				strong
					font-size 16px
					color #2c2434
					line-height 1.7
					font-weight normal
					white-space nowrap
					overflow hidden
					text-overflow ellipsis
					display block
				span
					font-size 14px
					color #686e76
					line-height 1.4
					white-space nowrap
					overflow hidden
					text-overflow ellipsis
					display block					


.mfzssz_page
	display table
	margin 0 auto 55px
	span
		height 36px
		border 1px solid #d2d2d2
		line-height 36px
		font-size 13px
		padding 0 12px
		display inline-block
		margin 0 5px
		transition .3s
		cursor pointer
		&:hover:not(.bdn)
			background #e50113
			color #fff
		&.acti
			background #e50113
			color #fff
		&.bdn
			border-color #fff
		&.white
			background #fff !important
			color #565d63 !important
		&.ac
			background #5d6770 !important
			color #fff !important
	&.two
		span
			background #909aa3
			color #fff
			&:hover
				background #5d6770
				color #fff
.mfzssz_about
	margin 40px auto 70px
	padding-bottom 30px
	border-bottom 1px solid #ededed
	.pic
		width 100%
		height 330px
		border 1px solid #d3d3d3
		padding 5px
		overflow hidden
		a
			width 100%
			height 100%
			display block
			&:hover
				img
					transform scale(1.1)
					opacity .85
			img
				width 100%
				min-height 100% 
				transition .3s
	.text
		padding-top 10px
		strong
			font-size 20px
			color #333233
			font-weight normal
			display block
			line-height 1.4
			margin-bottom 20px
		p
			font-size 14px
			color #5b5d61
			text-align justify
			line-height 1.9
			b
		

.mfzssz_we-hj
	.pic
		height 368px
		height 245px
		border 1px solid #ccc
		overflow hidden
		margin-bottom 50px
		a
			width 100%
			height 100%
			display block
			&:hover
				img
					transform scale(1.1)
					opacity .85
			img
				width 100%
				min-height 100%
				transition .3s


.mfzssz_new-list
	overflow hidden
	margin-bottom 60px
	ul
		li
			width 100%
			padding-bottom 25px
			border-bottom 1px dashed #eee
			margin-bottom 25px
	.pic
		width 212px
		height 160px
		overflow hidden
		float left
		a
			width 100%
			height 100%
			display block
			img
				width 100%
				min-height 100%
	.text
		width calc(100% - 260px)
		float right
		strong
			font-size 18px
			color #1c1f24
			display block
			font-weight normal
			line-height 1.4
			margin-bottom 5px
		span
			font-size 13px
			color #818882
			margin-bottom 15px
			display block
		p
			height 46px
			text-indent 20px
			font-size 14px
			text-align justify
			line-height 1.7
			overflow: hidden;-ms-text-overflow: ellipsis;text-overflow: ellipsis;
			-webkit-line-clamp:2;-webkit-box-orient:vertical;display:-webkit-box;word-wrap:break-word;
		a
			width 112px
			height 36px
			line-height 36px
			display block
			text-align center
			color #fff
			font-size 14px
			background #5e6770
			transition .3s
			&:hover
				opacity .85


.mfzssz_news-box
	margin 45px auto 450px
	overflow hidden
	.sidebar
		width 220px
		float left
		.box
			width 100%
			margin-bottom 50px
			&.two
				background #e7e7e7
				padding-bottom 10px
				strong
					margin-bottom 10px
			strong
				background #42454a
				line-height 48px
				display block
				color #fff
				font-size 18px
				text-align center
				margin-bottom 5px
				font-weight normal
			a
				position relative
				width 100%
				height 40px
				line-height 40px
				background #e7e7e7
				font-size 14px
				color #52534d
				display block
				margin-bottom 5px
				transition .3s
				padding-left 40px
				&:before
					content ""
					position absolute
					top 50%
					left 10px
					transform translateY(-50%)
					width 5px
					height 5px
					background #666
					display block
				&:hover
					background #d2d0d0
					text-decoration none
				&:active
					background #d2d0d0
			p
				font-size 14px
				text-align justify
				line-height 1.7
				color #52534d
				margin-bottom 10px
				padding 0 10px
	.mfzssz_article
		width calc(100% - 285px)
		float right
		.top
			width 100%
			display table
			height 43px
			line-height 43px
			border-bottom 1px solid #4a4b4d
			margin-bottom 25px
			strong
				font-size 18px
				color #62676b
				float left
			.right
				float right
				font-size 12px
				color #000002
				a
					color #4e5255
		.tou
			width 100%
			display table
			border-bottom 1px dashed #c5c5c5
			margin-bottom 25px
			padding-bottom 15px
			b
				font-size 22px
				display block
				text-align center
				font-weight normal
				margin-bottom 15px
				line-height 1.4
			span
				font-size 13px
				color #9c9d9f
				display block
				text-align center
				line-height 1.4
				i
					width 20px
					display inline-block
		.text
			padding 0 20px 20px
			border-bottom 1px solid #c5c5c5
			margin-bottom 20px
			p
				font-size 14px
				text-align justify
				line-height 1.7
				text-indent 30px
				margin-bottom 10px
				color #000
		.turn
			width 100%
			display table
			margin-bottom 10px
			span
				color #333 
				font-size 14px 
				line-height 1.7 
				margin-right 35px
				em
					float left 
				a
					width 160px
					color #4a5456
					white-space nowrap
					overflow hidden
					text-overflow ellipsis
					display inline-block


.mfzssz_recommend
	strong
		height 42px
		line-height 42px
		background #484b50
		font-size 18px
		color #fff
		padding-left 15px
		display block
		font-weight normal
	.list-li
			width 100%
			display table
			border 1px solid #d6d6d6
			padding 20px 15px 15px
			ul
				li
					width 49%
					border-bottom 1px dashed #adadad
					line-height 35px
					&:nth-child(even)
						float right
					a
						position relative
						font-size 14px
						color #4b5151
						width 80%
						float left
						white-space nowrap
						overflow hidden
						text-overflow ellipsis
						padding-left 20px
						&:before
							content ""
							position absolute
							top 15px
							left 8px
							width 3px
							height 3px
							background #868686
							display block
					span
						float right
						font-size 14px
						color #9a9899
		
	
.mfzssz_case
	margin 40px auto 30px
	overflow hidden
	.left
		width 72.5%
		height 730px
		overflow hidden
		float left
		.pic-box
			position relative
			width 100%
			height 587px
			margin-bottom 20px
			ul
				li
					position absolute
					width 100%
					height 587px
					overflow hidden
					img
						width 100%
						min-height 100%
			i
				position absolute
				z-index 3
				top 50%
				transform translateY(-50%)
				width 57px
				height 90px
				cursor pointer
				opacity .85
				&.pr
					left 33px
				&.nt
					right 33px
				img
					width 100%
		.pic-switch
			position relative
			width 100%
			height 114px
			background #e6e6e6
			text-align center
			.box
				position relative
				width 90px
				height 80px
				overflow hidden
				display inline-block
				margin 12px 10px 12px
				border 1px solid #fff
				border-radius 3px
				cursor pointer
				&.acti
					border-color #f00
				img
					width 100%
					min-height 100%
				span
					position absolute
					top 1px
					left 1px
					height 20px
					padding 0 10px
					font-size 12px
					color #fff
					background rgba(#000,.7)
					line-height 20px
			i
				position absolute
				top 50%
				width 33px
				height 54px
				transform translateY(-50%)
				cursor pointer
				&.pr
					left 40px
				&.nt
					right 40px
	.right
		width calc(100% - (72.5%+20px))
		float right
		border 1px solid #e3e3e3
		height 730px
		overflow hidden
		padding 15px 25px
		strong
			font-size 23px
			color #000
			font-weight normal
			display block
			line-height 1.4
			border-bottom 1px solid #e2e2e2
			padding-bottom 10px
			margin-bottom 20px
		p
			font-size 15px
			color #5f6062
			text-align justify
			line-height 1.7
			margin-bottom 10px
			&:last-of-type
				margin-bottom 20px
			span
				color #1d2220
				font-weight bold
		a
			width 95%
			height 55px
			line-height 55px
			background #97292c
			border-radius 3px
			text-align center
			font-size 26px
			color #fff
			display block
			margin 0 auto
			transition .3s
			&:hover
				text-decoration none
				opacity .85
				

.mfzssz_related-case
	background #f5f5f5
	padding 40px 0 60px
	h2
		font-size 36px
		color #000
		font-weight normal
		margin 0 0 60px 0
		text-align center
		

